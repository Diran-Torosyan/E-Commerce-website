<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.clothingstore.controller.HomePageController">
    <TextField fx:id="searchBar" promptText="Search..." />
    <Button text="Search" onAction="#handleSearch" />
    <Button text="Filter" fx:id="filterButton" onAction="#handleFilter" />
    <Button text="Account" fx:id="accountButton" onAction="#handleAccount" />
    <Button text="Cart" fx:id="cartButton" onAction="#handleCart" />
    <GridPane fx:id="itemsContainer" hgap="10" vgap="10" />
</VBox>
